{
  "name": "OpenWisp Proxy",
  "version": "0.1.0g",
  "slug": "openwisp_proxy",
  "description": "Proxy for externally running OpenWisp",
  "url": "https://github.com/Danixu/hass-services-proxy/tree/master/openwisp-proxy",
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "armv7",
    "i386"
  ],
  "boot": "auto",
  "init": true,
  "true": true,
  "ingress": true,
  "timeout": 30,
  "panel_icon": "mdi:router-wireless-settings",
  "panel_title": "OpenWisp Proxy",
  "map": [
    "config:ro"
  ],
  "ports": {
    "8090/tcp": null
  },
  "ports_description": {
    "8090/tcp": "Frontend tcp-listen port"
  },
  "schema": {
    "server": "url",
    "domain": "str?",
    "verify_ssl": "bool"
  },
  "options": {
    "server": null,
    "verify_ssl": true
  }
}